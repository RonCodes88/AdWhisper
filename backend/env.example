# AdWhisper Backend - Environment Configuration
# Copy this file to .env and fill in your actual values

# ==================== ChromaDB Configuration ====================
CHROMA_DB_PATH=.chroma_db

# ==================== ASI:ONE LLM Agent Addresses ====================
# These are the official ASI:ONE agent addresses (rate limited to 6 requests/hour)
ASI_OPENAI_AGENT=agent1q0h70caed8ax769shpemapzkyk65uscw4xwk6dc4t3emvp5jdcvqs9xs32y
ASI_CLAUDE_AGENT=agent1qvk7q2av3e2y5gf5s90nfzkc8a48q3wdqeevwrtgqfdl0k78rspd6f2l4dx

# ==================== API Keys ====================
# Agentverse API Key (for agent deployment)
AGENTVERSE_API_KEY=your_agentverse_api_key_here

# ASI:ONE API Key (for ASI agent integration)
ASI1_API_KEY=your_asi1_api_key_here

# ==================== Agent Addresses ====================
# These will be populated after running each agent for the first time
# Copy the addresses from the agent startup logs

# Ingestion Agent
INGESTION_AGENT_ADDRESS=

# Text Bias Agent
TEXT_BIAS_AGENT_ADDRESS=

# Visual Bias Agent
VISUAL_BIAS_AGENT_ADDRESS=

# Scoring Agent
SCORING_AGENT_ADDRESS=

# ==================== Server Configuration ====================
BACKEND_PORT=8000
FRONTEND_URL=http://localhost:3000

# ==================== Model Configuration ====================
# Text embedding model (sentence-transformers)
TEXT_EMBEDDING_MODEL=all-MiniLM-L6-v2

# Visual embedding model (CLIP)
VISUAL_EMBEDDING_MODEL=ViT-B-32

# ==================== Logging ====================
LOG_LEVEL=INFO

ANTHROPIC_API_KEY=sk-ant-